<appian-plugin name="Guidewire" key="com.appian.guidewire">
  <plugin-info>
    <description>Guidewire Connected System Plugin</description>
    <vendor name="Appian" url="https://appian.com/"/>
    <version>1.0.0.0</version>
    <application-version min="7.6.0.4"/>
  </plugin-info>
  <connected-system key="Guidewire">
    <version major-version="1">
      <connected-system-template key="GuidewireCSP"
                                 name="Guidewire"
                                 class="com.appian.guidewire.templates.GuidewireCSP"/>
      <integration-template key="GuidewireIntegrationTemplate"
                            name="Guidewire"
                            class="com.appian.guidewire.templates.apis.GuidewireIntegrationTemplate"
                            request-policy="read_and_write" />
    </version>
  </connected-system>
</appian-plugin>
